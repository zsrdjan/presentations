{
  "ConnectionStrings": {
    "MainDbContext": "Server=.;Database=Demo;Trusted_Connection=True;MultipleActiveResultSets=true"
  },
  "App": {
    "GitHubApi": {
      "SuperSecretKey": "DeveloperSecret"
    }
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug"
    },
    "WriteTo": [
      {
        "Name": "RollingFile",
        "Args": {
          "pathFormat": "c:/LogFiles/crafters-cloud-key-vault-demo-api-{Date}.log"
        }
      }
    ]
  },
  "ApplicationInsights": {
    "InstrumentationKey": ""
  }
}